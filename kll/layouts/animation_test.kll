A[testanimation, 3] <= P[1](60,90,140);

# Pixel clear test (scancodes)
A[clear_pixels] <= loops:1;
A[clear_pixels, 1] <=
	S[18](0,0,0),
	S[19](0,0,0),
	S[20](0,0,0),
	S[29](0,0,0),
	S[38](0,0,0);

# Rainbow, static interpolation test
