A[rainbow_static_fill_interp, 1] <=
	P[c:0%](255,0,0),
	P[c:25%](255,255,0),
	P[c:50%](0,255,0),
	P[c:75%](0,0,255),
	P[c:100%](127,0,255);

# Full fill example
A[blue_fill_interp] <= pfunc:interp;
A[blue_fill_interp, 1] <=
	P[c:0%](0x00,0xAE,0xDA),
	P[c:100%](0x00,0xAE,0xDA);

